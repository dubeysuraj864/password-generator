{
  "manifest_version": 3,
  "version": "1.0",
  "name": "Password generator",
  "action": {
      "default_popup": "index.html",
      "default_icon": "icon.png"
  }
  "content_security_policy": {
    "script-src": "self unsafe-eval",
    "unsafe-eval": "object-src self",
    "unsafe-inline": "self"
}
}